{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": ["airbnb/base"],
  "rules": {
    "import/prefer-default-export": "off",
    "no-useless-escape": "off",
    "no-use-before-define": "off",
    "func-names": "off",
    "max-len": ["error", 150, {"ignoreUrls": true}]
  }
}
